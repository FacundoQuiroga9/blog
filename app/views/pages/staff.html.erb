<div class="container">
  <h1 class="my-5">Staff</h1>
  <div class="row justify-content-center">
    <% @staff_members.each do |member|%>
    <div class="col-2 my-3">
      <div class="card-member <%= "#{member[:class]}" %>">
      <div class="card-member-info">
        <h4><%= member[:name] %></h4>
        <h6><%= member[:position] %></h6>
      </div>
    </div>
    </div>
  <% end %>
  </div>
</div>
